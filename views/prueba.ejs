    <div class="separador"></div>
    <style>.separador{
        margin-top: 05%;
        }
    </style>
    <div class="container">
        <div class="row">
            <% for(var item in productos) {%>
                <%if (productos[item].stock) {%>
                        <div class="col">
                            <div class="card" style="width: 20rem;height: 25 rem;">
                                <img class = "<%= productos[item].name%>" src="<%= productos[item].img %>"alt="" width="300" height="300"></img>
                                <div class="card-body">
                                    <h5 class="card-title"><%= productos[item].name %> </h5>
                                    <div class="card" style="width: 18rem;">
                                        <ul class="list-group list-group-flush">
                                          <li class="list-group-item">tipo:<%=productos[item].tipo %></li>
                                          <li class="list-group-item">precio:<%=productos[item].precio %></li>
                                          <li class="list-group-item">Stock:<%=productos[item].stock %></li>
                                        </ul>
                                      </div>
                                </div> 
                            </div>
                        </div>
                <% } else{ %>
                    <div class="col">
                        <div class="card" style="width: 20rem;height: 25 rem;">
                            <img class= "imagenY" src="<%= productos[item].img %>"alt="" width="300" height="300"></img>
                            <style>.imagenY {
                                filter: grayscale(100%);
                            }</style>
                            <div class="card-body">
                                <h5 class="card-title"><%= productos[item].name %> </h5>
                                <div class="card" style="width: 18rem;">
                                    <ul class="list-group list-group-flush">
                                      <li class="list-group-item">tipo:<%=productos[item].tipo %></li>
                                      <li class="list-group-item">precio:<%=productos[item].precio %></li>
                                      <li class="list-group-item">Stock:<%=productos[item].stock %></li>
                                    </ul>
                                  </div>
                            </div> 
                        </div>
                    </div>
                <% }; %>
            <% }; %>
        </div>
    </div>
